  },
  "gitHead": "c856d01d5723ca07470fa146af0281f01472ea55",
  "private": false,
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist && rm -rf tsconfig.tsbuildinfo",
    "compile:tsup": "yarn clean && tsc --emitDeclarationOnly --outDir './dist/types' & tsup --legacy-output",
    "compile": "tsc --emitDeclarationOnly --outDir './dist/types' & tsc --project tsconfig.cjs.json & tsc --project tsconfig.esm.json && cp \"./package.json\" ./dist/",
    "compile:submodules": "rollup -c",
    "build": "yarn clean && yarn compile && yarn compile:submodules && yarn post:build",
    "post:build": "node ../../scripts/subModulePostBuild.mjs --ignoreFiles '[\"types.ts\",\"theme.ts\"]'",
    "prepack": "yarn build",
    "start": "tsdx watch",
    "test": "echo 'removed default test' ",
    "alt-test": "tsc test --env=jsdom",
    "test:watch": "tsc test --env=jsdom --watchAll",
    "test:ci": "yarn alt-test --coverage --no-watch --watch=false --watchAll=false --no-watchman --forceExit --detectOpenHandles",
    "pre-commit": "pretty-quick --staged && yarn test:ci && yarn build",
    "lint": "prettier --write './src/**/*' --ignore-path '../../.prettierignore'",
    "cosmos": "cosmos",
    "cosmos:export": "cosmos-export",
    "depcheck": "npx npm-check"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@rollup/plugin-image": "^3.0.1",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^13.4.0",
    "@types/lodash.clamp": "^4.0.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-hash-link": "^2.4.5",
    "@types/rebass": "^4.0.10",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "pretty-quick": "^2.0.1",
    "react": "^18.2.0",
    "react-cosmos": "^5.7.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.7.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-svg": "^2.0.0",
    "styled-components": "^5.3.6",
    "tslib": "^2.4.1",
    "tsup": "^7.2.0"
  }
