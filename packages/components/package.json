  },
  "author": "pas3lle",
  "license": "MIT",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/file-mock.js"
    },
    "testMatch": [
      "<rootDir>src/**/__tests__/*.{ts,tsx}"
    ]
  },
  "peerDependencies": {
    "react": ">=16",
    "react-ga": ">=3",
    "react-router-dom": ">=6",
    "styled-components": ">=5"
  },
  "dependencies": {
    "@past3lle/assets": "^0.0.23-alpha.1",
    "@past3lle/constants": "^0.1.10-alpha.1",
