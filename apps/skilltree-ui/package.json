{
  "name": "@past3lle/skilltree-ui",
  "description": "SKILLTREE UI",
  "homepage": ".",
  "sideEffects": false,
  "main": "build/skilltree.js",
  "module": "build/skilltree.esm.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PAST3LLE/past3lle-monorepo.git"
  },
  "private": true,
  "version": "0.0.20",
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "npx prettier --write src --ignore-path ./.prettierignore --config ./.prettierrc",
    "start:mock": "REACT_APP_IS_MOCK=true && yarn start",
    "start": "PORT=6969 GENERATE_SOURCEMAP=false craco start",
    "build": "craco build",
    "build:serve": "DISABLE_ESLINT_PLUGIN='true' craco build && npx serve build -l 666",
    "test": "craco test --env=jsdom",
    "test:debug": "craco --inspect-brk test --runInBand --no-cache",
    "dedup": "npx yarn-deduplicate",
    "copy:metadata": "cp ../skills-nft/src/skills/metadata.js ./src/mock/metadata/fullMetadata.js"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@craco/craco": "^5.7.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.5",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "@types/rebass": "^4.0.5",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "abitype": "^0.3.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jotai-devtools": "^0.1.0",
    "prettier": "^2.8.3",
    "react-error-overlay": "^6.0.11",
    "react-scripts": "^5.0.1",
    "serve": "^11.3.2",
    "source-map-explorer": "^2.5.3",
    "typescript": "^4.8.3",
    "web-vitals": "^3.1.0"
  },
  "dependencies": {
    "@past3lle/assets": "^0.0.9",
    "@past3lle/components": "^0.0.26",
    "@past3lle/constants": "^0.0.16",
    "@past3lle/hooks": "^0.0.11",
    "@past3lle/skills-nft": "^0.0.5",
    "@past3lle/skilltree-contracts": "^0.0.1-rc.3",
    "@past3lle/theme": "^0.0.21",
    "@past3lle/types": "^0.0.11",
    "@past3lle/utils": "^0.0.16",
    "@web3modal/ethereum": "^2.1.0",
    "@web3modal/react": "^2.1.0",
    "ethers": "^5.7.2",
    "jotai": "^1.13.1",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "rebass": "^4.0.7",
    "styled-components": "^5.3.5",
    "wagmi": "^0.11.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
