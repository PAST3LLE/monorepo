{
    "compilerOptions": {
        "jsx": "react",
        "baseUrl": "./packages",
        "paths": {
            "@past3lle/assets/*": [
                "assets/src/*"
            ],
            "@past3lle/components/*": [
                "components/src/*"
            ],
            "@past3lle/constants/*": [
                "constants/src/*"
            ],
            "@past3lle/hooks/*": [
                "hooks/src/*"
            ],
            "@past3lle/skilltree-ui/*": [
                "skilltree-ui/src/*"
            ],
            "@past3lle/theme/*": [
                "theme/src/*"
            ],
            "@past3lle/types/*": [
                "types/src/*"
            ],
            "@past3lle/utils/*": [
                "utils/src/*"
            ]
        }
    },
    "references": [
        {
            "path": "packages/assets"
        },
        {
            "path": "packages/components"
        },
        {
            "path": "packages/constants"
        },
        {
            "path": "packages/hooks"
        },
        {
            "path": "packages/skilltree-ui"
        },
        {
            "path": "packages/theme"
        },
        {
            "path": "packages/types"
        },
        {
            "path": "packages/utils"
        }
    ],
    "files": []
}