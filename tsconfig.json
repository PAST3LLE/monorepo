{
    "compilerOptions": {
        "jsx": "react",
        "baseUrl": "./packages",
        "paths": {
            "@past3lle-schematics/assets/*": [
                "assets/src/*"
            ],
            "@past3lle-schematics/components/*": [
                "components/src/*"
            ],
            "@past3lle-schematics/constants/*": [
                "constants/src/*"
            ],
            "@past3lle-schematics/hooks/*": [
                "hooks/src/*"
            ],
            "@past3lle-schematics/theme/*": [
                "theme/src/*"
            ],
            "@past3lle-schematics/types/*": [
                "types/src/*"
            ],
            "@past3lle-schematics/utils/*": [
                "utils/src/*"
            ]
        }
    },
    "files": [],
    "references": [
        {
            "path": "packages/assets"
        },
        {
            "path": "packages/components"
        },
        {
            "path": "packages/constants"
        },
        {
            "path": "packages/hooks"
        },
        {
            "path": "packages/theme"
        },
        {
            "path": "packages/types"
        },
        {
            "path": "packages/utils"
        }
    ]
}